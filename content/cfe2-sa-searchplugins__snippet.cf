
${masterfiles}/desktops/public/etc/firefox/searchplugins
	dest=/etc/iceweasel/searchplugins
	server=${server}
	r=inf
	ignore=.git
	purge=true
	backup=false
	owner=root
	group=root
	mode=640
	type=checksum
	define=purge_localram__firefox

shellcommands:

	purge_localram__firefox::
        "/bin/sh -c 'killall -9 firefox ; killall -9 iceweasel'"

